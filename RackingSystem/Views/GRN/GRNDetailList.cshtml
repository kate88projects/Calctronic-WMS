            //     success: function (result) {
            //         if (result.success) {
            //             const today = new Date();
            //             const years = result.data.expiredYears;
            //             const months = result.data.expiredMonths;

            //             const expiredDate = new Date(today);
            //             expiredDate.setFullYear(expiredDate.getFullYear() + years);
            //             expiredDate.setMonth(expiredDate.getMonth() + months);
            //             const formattedDate = expiredDate.toISOString().split('T')[0];

            //             document.getElementById("dtpExDate").value = formattedDate;
            //         }
            //     },
