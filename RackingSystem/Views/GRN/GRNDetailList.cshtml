@section PageHeader {
    <header class="d-flex align-items-center justify-content-between w-100">
        <h2 class="pageTitle">GRN List <b></b></h2>
    </header>
}

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div id="contentBox" class="row mt-2" style="padding:5px;">
                        <div class="col-sm-2" style="">
                            <input id="txtGRN" name="GRNBatchNo" type="text" class="form-control" style="" placeholder="GRN No" />
                        </div>
                        <div class="col-sm-2" style="">
                            <input id="txtItemCode" name="ItemCode" type="text" class="form-control" style="" placeholder="Item" />
                        </div>
                        <div class="col-sm-2" style="">
                            <input id="txtReelCode" name="ReelCode" type="text" class="form-control" style="" placeholder="Reel Code" />
                        </div>
                        <div class="col-sm-5" style="">
                            <div class="row">
                                <div class="col-4" style="padding:0;">
                                    <select id="selDateType" name="DateType" class="form-control">
                                        <option value="C">Created Date</option>
                                        <option value="U">Updated Date</option>
                                    </select>
                                </div>
                                <div class="col-4" style="padding:0;">
                                    <input id="dtpDateFrom" name="DateFrom" type="date" class="form-control" placeholder="Date From" />
                                </div>
                                <div class="col-4" style="padding:0;">
                                    <input id="dtpDateTo" name="DateTo" type="date" class="form-control" placeholder="Date To" />
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-1" style="">
                            <button type="button" class="btn btn-primary" style="width:100%;" onclick="loadList(1)"><i class="bi bi-search"></i> </button>
                        </div>
                    </div>
                    <hr />
                    <div class="table-filter">
                        <div class="row" style="padding-left:5px;">
                            <div class="col-sm-12">
                                <div class="show-entries">
                                    <label>Show</label>
                                    <select id="selpageSize" name="pageSize" class="form-control">
                                        <option value="10">10</option>
                                        <option value="25">25</option>
                                        <option value="50">50</option>
                                        <option value="100">100</option>
                                        <option value="200">250</option>
                                    </select>
                                    <span>entries</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="text-nowrap table-container mt-2">
                        <table id="listTable" class="row-border dataTable hover order-column" style="width:100%">
                            <thead>
                                <tr>
                                    <th>GRN No</th>
                                    <th>Item</th>
                                    <th>Qty</th>
                                    <th>Reel Code</th>
                                    @* <th>Reel Status</th> *@
                                    <th>Created Date</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>

                    <div class="dt-container dt-empty-footer">
                        <div class="dt-layout-row">
                            <div class="hint-text">Showing <b id="lblCurCount">0</b> of <b id="lblTotal">0</b> entries</div>
                            @* <ul class="pagination" id="ulPagination">
                        </ul> *@
                            <div class="dt-layout-cell dt-layout-end">
                                <div class="dt-paging">
                                    <nav aria-label="pagination" id="ulPagination">
                                    </nav>
                                </div>
                            </div>
                            <input type="hidden" id="txtCurPage">
                            <input type="hidden" id="txtTotalRecord">
                            <input type="hidden" id="txtPageSize">
                            <input type="hidden" id="txtTotalPage">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" data-bs-backdrop="static" id="myEditModal" role="dialog">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Update GRN</h4>
            </div>
            <div class="modal-body">
                <div class="mb-3">
                    <label>Supplier Name</label>
                    <input id="txtSupplierName2" type="text" class="form-control" style="" placeholder="" onclick="" maxlength="255" oninput="removeErrMsg(this.id)" />
                    <div id="txtSupplierName2Error" class="error-message"></div>
                </div>
                <div class="mb-3">
                    <label>Supplier Ref No</label>
                    <input id="txtSupplierRefNo2" type="text" class="form-control" style="" placeholder="" maxlength="50" oninput="removeErrMsg(this.id)" />
                    <div id="txtSupplierRefNo2Error" class="error-message"></div>
                </div>
                <div class="mb-3">
                    <label>Item</label>
                    <input id="txtItem2" type="button" class="form-control" style="" placeholder="" onclick="showItemForm('Edit')" />
                    <div id="txtItem2Error" class="error-message"></div>
                </div>
                <div class="mb-3">
                    <label>Qty</label>
                    <input id="txtQty2" type="number" class="form-control" style="" placeholder="" oninput="removeErrMsg(this.id)" />
                    <div id="txtQty2Error" class="error-message"></div>
                </div>
                <div class="mb-3">
                    <label>Expiry Date</label>
                    <input id="dtpExDate2" name="DateFrom" type="date" class="form-control" placeholder="Expiry Date" oninput="removeErrMsg(this.id)" />
                    <div id="dtpExDate2Error" class="error-message"></div>
                </div>
                <div class="mb-3">
                    <label>Remark</label>
                    <input id="txtRemark2" type="text" class="form-control" style="" placeholder="" maxlength="255" />
                    <div id="txtRemark2Error" class="error-message"></div>
                </div>

                <input type="hidden" id="txtGRN2">
                <input type="hidden" id="txtId2">
                <input type="hidden" id="txtItemId2">
                <input type="hidden" id="txtLoaderId2">
                <input type="hidden" id="txtReelCode2">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="updateReceive()">Save</button>
                <button type="button" class="btn btn-outline-secondary" onclick="cancelEditForm()">Cancel</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="itemModal" role="dialog" style="background-color: rgba(0, 0, 0, 0.2);">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-success text-white">
                <h4 class="modal-title">Select Item</h4>
            </div>
            <div class="modal-body">
                <table id="itemTable" class="row-border dataTable hover order-column mb-2 mt-2" style="width:100%">
                    <thead>
                        <tr>
                            <th>Item Code</th>
                            <th>UOM</th>
                            <th>Description</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
                <input type="hidden" id="txtItemStatus">
            </div>
        </div>
    </div>
</div>


@section Scripts
{
    <link rel="stylesheet" href="~/css/sweetalert2.min.css">
    <link rel="stylesheet" href="~/css/dataTables.dataTables.css">
    <link rel="stylesheet" href="~/css/columnControl.dataTables.css">

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="../js/dataTables.js"></script>
    <script src="../js/dataTables.columnControl.js"></script>
    <script src="../js/columnControl.dataTables.js"></script>
    <script src="~/js/sweetalert2.all.min.js"></script>

    <script type="text/javascript">

        $(document).ready(function () {
            $('#txtTotalRecord').val(0);
            $('#txtTotalPage').val(1);

            var now = new Date();

            var day = ("0" + now.getDate()).slice(-2);
            var month = ("0" + (now.getMonth() + 1)).slice(-2);

            var today = now.getFullYear()+"-"+(month)+"-"+(day) ;

            $('#dtpDateFrom').val(today);
            $('#dtpDateTo').val(today);

            loadList(1);
            loadItemList();
        });

        function prevPage() {
            var curPage = $("#txtCurPage").val();
            if (curPage == 1) {
                return;
            }
            loadList(curPage - 1);
        }

        function nextPage() {
            var curPage = Number($("#txtCurPage").val());
            var ttl = Number($("#txtTotalRecord").val());
            var pageSize = Number($("#txtPageSize").val());
            var ttlPage = Number($("#txtTotalPage").val());
            // var ttlPage = ttl / pageSize;
            // if ((ttl % pageSize) > 0) {
            //     ttlPage = ttlPage + 1;
            // }
            if ((curPage + 1) > ttlPage) {
                return;
            }
            loadList(curPage + 1);
        }

        function loadItemList() {
            $.ajax({
                url: "/Item/GetActiveItemList/",
                type: "GET",
                dataType: 'JSON',
                contentType: 'application/json; charset=utf-8',
                beforeSend: function () {
                    Swal.fire({
                        text: 'Please Wait',
                        width: '300px',

                        allowEscapeKey: false,
                        allowOutsideClick: false,
                        showConfirmButton: false,
                    });
                    Swal.showLoading();
                },
                success: function (result) {
                    swal.close();
                    if (result.success == true) {
                        $('#itemTable').DataTable( {
                            dom: 'frtip',
                            ordering: false,
                            data: result.data,
                            paging: true,
                            pageLength: 25,
                            lengthMenu: [25, 50, 100, 250, 500],
                            columns: [
                                { data: 'itemCode' },
                                { data: 'uom' },
                                {
                                    data: null,
                                    render: function (data, type, row, meta) {
                                        return '<p><span>' + row.description + '</span><br/>' +
                                        '<span style="font-size:13px;color:grey;">' + row.desc2 + '</span></p>';
                                    }
                                },
                                {
                                    data: null,
                                    render: function (data, type, row, meta) {
                                        const rowData = JSON.stringify(row).replace(/"/g, "&quot;");
                                        return `<div style="display:flex;">
                                                    <button class="btn btn-success d-grid addItem-btn" data-row="${rowData}">
                                                        <span><i class="bi bi-plus""></i> Add</span>
                                                    </button>
                                                </div>`;
                                    }
                                }
                            ],
                            language: {
                                searchPlaceholder: "🔍︎ Search",
                                search: ""
                            },
                            ordering: {
                                indicators: false
                            },
                            columnControl: ['orderStatus'],
                            columnDefs: [
                                { width: '15%', targets: 0 },
                                { width: '10%', targets: 1 },
                                { width: '60%', targets: 2 },
                                { width: '15%', targets: 3 }
                            ]
                        });

                    }
                    else {
                        swal.fire({
                            icon: 'error',
                            title: "Wait",
                            text: result.errMessage
                        });
                    }
                },
                error: function (error) {
                    swal.fire({
                        icon: 'error',
                        title: "Wait",
                        text: error.val
                    });
                }
            });
        }

        function loadList(page) {
            $('#txtCurPage').val(page);
            $('#txtPageSize').val($("#selpageSize").val());
            var ttlPage = $("#txtTotalPage").val();
            for (var j = 1; j <= ttlPage; j++) {
                $('#btnP' + j).removeClass('current');
            }
            $('#btnP' + page).addClass('current');

            var data = {
                GRNBatchNo: $("#txtGRN").val(),
                ItemCode: $("#txtItemCode").val(),
                ReelCode: $("#txtReelCode").val(),
                DateType: $("#selDateType").val(),
                DateFrom: $("#dtpDateFrom").val(),
                DateTo: $("#dtpDateTo").val(),
                page: page,
                pageSize: $("#selpageSize").val(),
            }

            $.ajax({
                url: "/GRN/GetGRNDetailList/",
                type: "POST",
                data: JSON.stringify(data),
                dataType: 'JSON',
                contentType: 'application/json; charset=utf-8',
                beforeSend: function () {
                    Swal.fire({
                        text: 'Please Wait',
                        width: '300px',
                        allowEscapeKey: false,
                        allowOutsideClick: false,
                        showConfirmButton: false,
                    });
                    Swal.showLoading()
                },
                success: function (result) {
                    // console.log(result);
                    swal.close()
                    if (result.success == true) {
                        $('#listTable').DataTable( {
                            data: result.data,
                            paging: false,
                            searching: false,
                            "info": false,
                            pageLength: $("#selpageSize").val(),
                            // lengthMenu: [50, 100, 250, 500, 1000],
                            "bDestroy": true,
                            columns: [
                                { data: 'grnBatchNo' },
                                {
                                    data: null,
                                    render: function (data, type, row, meta) {
                                        // return '<p>' + row.itemCode + '</p><p>' + row.itemDesc + '</p>';
                                        return '<p style="margin:0;"><span>' + row.itemCode + '</span><br/>' +
                                        '<span style="font-size:13px;color:grey;">' + row.itemDesc + '</span></p>';
                                    }
                                },
                                { data: 'qty' },
                                { data: 'reelCode' },
                                // {
                                //     data: null,
                                //     render: function (data, type, row, meta) {
                                //         return '<p style="margin:0;">' + row.reelStatus + '</p>';
                                //         // return '<p>' + row.createdDateDisplay + '</p><p>' + row.updatedDateDisplay + '</p>';
                                //     }
                                // },
                                {
                                    data: null,
                                    render: function (data, type, row, meta) {
                                        // if (row.createdDateDisplay == row.updatedDateDisplay) {
                                        //     return `<p style="margin:0;">${row.createdDateDisplay}</p>`;
                                        // }
                                        // else {
                                        //     return '<p style="margin:0;"><span>' + row.updatedDateDisplay + '</span><br/>' +
                                        //     '<span style="font-size:13px;color:grey;">' + row.createdDateDisplay + '</span></p>';
                                        // }
                                        return '<p>' + row.createdDateDisplay + '</p> ';
                                    }
                                },
                                {
                                    data: null,
                                    render: function (data, type, row, meta) {
                                        // console.log(row);
                                        const rowData = JSON.stringify(row).replace(/"/g, "&quot;");
                                        return `<button class="btn btn-warning edit-btn" data-row="${rowData}" id="editBtn" data-bs-toggle="tooltip" title="Edit">
                                                <i class="bi bi-pencil-square"></i> Edit</button>
                                                <button class="btn btn-danger deactivate-account delete-btn" data-row="${rowData}" data-id="${row.grnDetail_Id}" data-code="${row.reelCode}"> Delete</button>`;
                                    }
                                }
                            ],
                        });

                        // if is page 1 then need regenerate pagination, else just show current entry
                        var curCnt = 0;
                        if (result.totalRecords > 0) {
                            curCnt = (page) * $("#selpageSize").val();
                            if (curCnt > result.totalRecords) {
                                curCnt = result.totalRecords;
                            }
                        }
                        $('#lblCurCount').html(curCnt);

                        if (page == 1) {
                            $('#lblTotal').html(result.totalRecords);
                            $('#ulPagination').html('');

                            var pageSize = $("#selpageSize").val();
                            var ttlPage = parseInt(result.totalRecords / pageSize);
                            if ((result.totalRecords % pageSize) > 0) {
                                ttlPage = ttlPage + 1;
                            }
                            $('#txtTotalRecord').val(result.totalRecords);
                            $('#txtTotalPage').val(ttlPage);

                        }

                        var isC = "";
                        var ttlPage = Number($("#txtTotalPage").val());
                        const myDiv = document.getElementById('ulPagination');
                        myDiv.innerHTML = "";
                        var pages = getVisiblePages(Number($('#lblTotal').html()), page, 8);

                        myDiv.innerHTML += '<button class="dt-paging-button disabled first" onclick="loadList(1)" role="link" type="button" aria-controls="listTable">«</button>';
                        myDiv.innerHTML += '<button class="dt-paging-button disabled" onclick="prevPage()" role="link" type="button" aria-controls="listTable">‹</button>';
                        // for (var i = 1; i <= ttlPage; i++) {
                        //     if (i == 1) {
                        //            myDiv.innerHTML += '<button class="dt-paging-button current" id="btnP'+ i +'" onclick="loadList('+ i +')" role="link" type="button" aria-controls="listTable">'+ i +'</button>';
                        //     }
                        //     else {
                        //            myDiv.innerHTML += '<button class="dt-paging-button" id="btnP'+ i +'" onclick="loadList('+ i +')" role="link" type="button" aria-controls="listTable">'+ i +'</button>';
                        //     }
                        //     lastPage = i;
                        // }
                        pages.forEach(function(val, idx) {
                            if (page == val) {
                                isC = "current";
                            }
                            else {
                                isC = "";
                            }
                            if (val <= ttlPage) {
                                myDiv.innerHTML += '<button class="dt-paging-button ' + isC + '" id="btnP'+ val +'" onclick="loadList('+ val +')" role="link" type="button" aria-controls="listTable">'+ val +'</button>';
                            }
                        });
                        myDiv.innerHTML += '<button class="dt-paging-button disabled" onclick="nextPage()" role="link" type="button" aria-controls="listTable">›</button>';
                        myDiv.innerHTML += '<button class="dt-paging-button last disabled" onclick="loadList(' + ttlPage + ')" role="link" type="button" aria-controls="listTable">»</button>';

                    }
                    else {
                        swal.fire({
                            icon: 'error',
                            title: "Wait",
                            text: result.errMessage
                        });
                    }
                },
                error: function (error) {
                    swal.fire({
                        icon: 'error',
                        title: "Wait",
                        text: error.val
                    });
                }
            });
        }

        function removeErrMsg(fields, clearValue = false) {
            if (!Array.isArray(fields)) {
                fields = [fields];
            }

            fields.forEach(function(field) {
                document.getElementById(field).classList.remove('is-invalid');
                document.getElementById(field).classList.remove('is-valid');
                document.getElementById(field + 'Error').textContent = '';
                if (clearValue) document.getElementById(field).value = '';
            });
        }

        function showItemForm(mode) {
            $('#txtItemStatus').val(mode);
            $('#itemModal').modal("show");
        }

        $('#itemTable').on('click', '.addItem-btn', function() {
            const row = JSON.parse($(this).attr('data-row'));
            addItemForm(row);
        });

        function addItemForm(row) {
            $("#txtItem2").val(row.itemCode);
            $("#txtItemId2").val(row.item_Id);

            // removeErrMsg('txtItem');
            $("#itemModal").modal("toggle");

