                        $("#txtCol4TotalReels").val(result.data.col4TotalReels);

                        $(`#percent-c1`).text(`${result.data.col1UsedPercentage}%`);
                        $(`#balance-c1`).text(`${result.data.col1TotalReels}`);
                        $(`#colHeight-c1`).text(`${result.data.colHeight}`);
