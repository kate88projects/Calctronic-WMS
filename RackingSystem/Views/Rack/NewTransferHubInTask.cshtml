            color: #673ab7;
        }

        .progressBar li::before {
            width: 50px;
            height: 50px;
            line-height: 50px;
            display: block;
            font-size: 20px;
            color: #fff;
            background: lightgrey;
            border-radius: 50%;
            margin: 0 auto 10px auto;
            padding: 2px;
        }

        .progressBar li::after {
            content: "";
            width: 100%;
            height: 2px;
            background: lightgrey;
            position: absolute;
            left: 50px;
            top: 25px;
            z-index: -1;
        }

        .progressBar li.active::before {
            background: #673ab7;
        }

        .progressBar li.next::after {
            background: #673ab7;
        }

        .progressBar #step1::before {
            content: "1";
